
@import "../../bootstrap.wxss";
.container{
  width: 100%;
  height: 5000px;
   background: #1DC7EA;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #1DC7EA), color-stop(100%, #4091ff));
    background: -webkit-linear-gradient(top, #1DC7EA 0%, #4091ff 100%);
    background: -o-linear-gradient(top, #1DC7EA 0%, #4091ff 100%);
    background: -ms-linear-gradient(top, #1DC7EA 0%, #4091ff 100%);
    background: linear-gradient(to bottom, #1DC7EA 0%, #4091ff 100%);
}

.container .button{
  margin-left: -100%
}
list{
  height:100%;
  width: 100%;
  
}


.list .item{
  width: 100%;
  height: 75px;
  display: flex;
  flex-direction:row;
  color: #FFFFFF;
  opacity: 1;
  background: rgba(255, 255, 255, 0.23);
  margin-top:10px;
  margin-bottom: 10px;
  border-radius: 10px;
  text-align: center;
}


.list .index{
  margin-top: 23px;
  text-align: center;
  color: greenyellow;
}



.list .center{
  display: inline-block;
  margin-top: 20px;
  width: 100%;
  font-size: 25px;
  overflow:hidden;
  white-space:nowrap;
  text-align: center;
}

